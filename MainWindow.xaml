        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rgr___BinTree_"
        mc:Ignorable="d"
        Title="Словарь терминов" Height="457" Width="800">
    <Grid Margin="0,0,0,-6">
        <ListView d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,10,514,10" Name="ListElements" SelectionChanged="ListElements_SelectionChanged">
        </ListView>
        <Button Name="CreateNewTerm" Content="Создать термин" HorizontalAlignment="Left" Margin="395,338,0,0" VerticalAlignment="Top" Height="63" Width="129" Click="CreateNewTerm_Click" RenderTransformOrigin="0.507,0.524"/>
        <TextBox Name="Description" HorizontalAlignment="Left" Margin="308,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="467" Height="281"/>

    </Grid>
</Window>
